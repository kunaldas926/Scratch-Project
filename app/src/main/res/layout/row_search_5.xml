<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="search5RowModel" type="com.application.app.modules.search3.data.model.Search5RowModel"/>
    </data>
    <FrameLayout android:id="@+id/frame2"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" style="@style/groupStyle">
        <LinearLayout android:id="@+id/linear1"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyle">
            <FrameLayout android:id="@+id/frame4"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" style="@style/groupStyle">
                <FrameLayout android:id="@+id/frame5"
                    android:layout_gravity="center"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <ImageView android:id="@+id/image"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_180px"
                        android:layout_width="@dimen/_120px"
                        android:scaleType="centerCrop"
                        cornerRadius="@{@dimen/_2px}" imageRes="@{@drawable/img_cardbox_33}"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:id="@+id/linear3"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_2px"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtTurnerHooch"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="@{search5RowModel.txtTurnerHooch}" style="@style/textStyleRobotoregular12"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtTomHanksRetur"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2px"
                    android:layout_width="match_parent"
                    android:text="@{search5RowModel.txtTomHanksRetur}" style="@style/textStyleRobotoregular12"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
