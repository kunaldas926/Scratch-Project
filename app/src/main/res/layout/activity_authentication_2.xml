<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="authentication2VM" type="com.application.app.modules.authentication2.data.viewmodel.Authentication2VM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24px"
                    android:layout_width="@dimen/_24px" android:src="@drawable/img_icon___arrowleft"/>
                <TextView android:id="@+id/txt"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8px"
                    android:layout_weight="0.02" android:layout_width="0dp"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:id="@+id/scrollView"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_10px"
            android:paddingEnd="@dimen/_16px" android:paddingStart="@dimen/_16px">
            <LinearLayout android:id="@+id/linear2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_900">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtLogIn"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_50px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_marginTop="@dimen/_32px"
                    android:layout_width="match_parent"
                    android:text="@{authentication2VM.authentication2Model.txtLogIn}" style="@style/textStyleRobotobold34"/>
                <TextView android:gravity="top|center"
                    android:id="@+id/txtPutYourEmail"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16px"
                    android:layout_marginStart="@dimen/_16px"
                    android:layout_marginTop="@dimen/_16px"
                    android:layout_width="match_parent"
                    android:text="@{authentication2VM.authentication2Model.txtPutYourEmail}" style="@style/textStyleRobotoregular20"/>
                <FrameLayout android:id="@+id/frame"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16px"
                    android:layout_marginStart="@dimen/_16px"
                    android:layout_marginTop="@dimen/_27px"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <FrameLayout android:id="@+id/frame1"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent" style="@style/groupStyle">
                        <LinearLayout android:id="@+id/linear3"
                            android:layout_gravity="center"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_8px"
                            android:layout_marginEnd="@dimen/_12px"
                            android:layout_marginStart="@dimen/_12px"
                            android:layout_marginTop="@dimen/_8px"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                            <LinearLayout android:id="@+id/linear4"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <TextView android:gravity="top|start"
                                    android:id="@+id/txtEmail"
                                    android:layout_gravity="top"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.1"
                                    android:layout_width="0dp"
                                    android:text="@{authentication2VM.authentication2Model.txtEmail}" style="@style/textStyleRobotoregular12_5"/>
                                <TextView android:gravity="top|start"
                                    android:id="@+id/txtDavid20Gmail"
                                    android:layout_gravity="bottom"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.51"
                                    android:layout_width="0dp"
                                    android:text="@{authentication2VM.authentication2Model.txtDavid20Gmail}" style="@style/textStyleRobotoregular16_1"/>
                                <ImageView android:id="@+id/image1"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_24px"
                                    android:layout_marginStart="@dimen/_50px"
                                    android:layout_width="@dimen/_24px" android:src="@drawable/img_icon___mail"/>
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                </FrameLayout>
                <FrameLayout android:id="@+id/frame2"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16px"
                    android:layout_marginStart="@dimen/_16px"
                    android:layout_marginTop="@dimen/_13px"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <FrameLayout android:id="@+id/frame3"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent" style="@style/groupStyle">
                        <LinearLayout android:id="@+id/linear5"
                            android:layout_gravity="center"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_8px"
                            android:layout_marginEnd="@dimen/_12px"
                            android:layout_marginStart="@dimen/_12px"
                            android:layout_marginTop="@dimen/_8px"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                            <LinearLayout android:id="@+id/linear6"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <TextView android:gravity="top|start"
                                    android:id="@+id/txtPassword"
                                    android:layout_gravity="top"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.18"
                                    android:layout_width="0dp"
                                    android:text="@{authentication2VM.authentication2Model.txtPassword}" style="@style/textStyleRobotoregular12_5"/>
                                <TextView android:gravity="top|start"
                                    android:id="@+id/txtLbl"
                                    android:layout_gravity="bottom"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.25"
                                    android:layout_width="0dp"
                                    android:text="@{authentication2VM.authentication2Model.txtLbl}" style="@style/textStyleRobotoregular16_1"/>
                                <View
                                    android:background="@color/indigo_500"
                                    android:id="@+id/line"
                                    android:layout_gravity="bottom"
                                    android:layout_height="@dimen/_18px"
                                    android:layout_marginStart="@dimen/_1px"
                                    android:layout_weight="0.01" android:layout_width="0dp"/>
                                <ImageView android:id="@+id/image2"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_24px"
                                    android:layout_marginStart="@dimen/_50px"
                                    android:layout_width="@dimen/_24px" android:src="@drawable/img_right_icon"/>
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                </FrameLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:clickable="true" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16px"
                    android:layout_marginStart="@dimen/_16px"
                    android:layout_marginTop="@dimen/_19px"
                    android:layout_width="match_parent"
                    android:text="@string/lbl_next"
                    android:textAllCaps="false" style="@style/textStyleRobotoregular14_1"/>
                <TextView android:gravity="top|center"
                    android:id="@+id/txtDonTHaveAnA"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16px"
                    android:layout_marginStart="@dimen/_16px"
                    android:layout_marginTop="@dimen/_100px"
                    android:layout_width="match_parent"
                    android:text="@{authentication2VM.authentication2Model.txtDonTHaveAnA}" style="@style/textStyleRobotoregular12"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
