<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="explore8VM" type="com.application.app.modules.explore8.data.viewmodel.Explore8VM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtExplore"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.18"
                    android:layout_width="0dp"
                    android:text="@{explore8VM.explore8Model.txtExplore}" style="@style/textStyleRobotoregular20"/>
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_width="@dimen/_18px" android:src="@drawable/img_icon___airplay"/>
                <ImageView android:id="@+id/image1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18px"
                    android:layout_marginStart="@dimen/_24px"
                    android:layout_width="@dimen/_18px" android:src="@drawable/img_icon___bell"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:id="@+id/scrollView"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_width="match_parent" android:paddingBottom="@dimen/_10px">
            <LinearLayout android:id="@+id/linear2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_900">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtCategoriesYou"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16px"
                    android:layout_marginTop="@dimen/_24px"
                    android:layout_width="match_parent"
                    android:text="@{explore8VM.explore8Model.txtCategoriesYou}" style="@style/textStyleRobotobold14"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16px"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_16px}"
                    style="@style/groupStyle" tools:itemCount="4" tools:listitem="@layout/row_explore_9"/>
                <LinearLayout android:id="@+id/linear3"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16px"
                    android:layout_marginStart="@dimen/_16px"
                    android:layout_marginTop="@dimen/_24px"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:gravity="top|start"
                        android:id="@+id/txtBestMovie"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.37"
                        android:layout_width="0dp"
                        android:text="@{explore8VM.explore8Model.txtBestMovie}" style="@style/textStyleRobotobold24"/>
                    <FrameLayout android:id="@+id/frame"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_50px"
                        android:layout_weight="0.38"
                        android:layout_width="0dp" style="@style/groupStyle">
                        <FrameLayout android:clickable="true"
                            android:id="@+id/frame1"
                            android:layout_gravity="center"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <FrameLayout android:id="@+id/frame2"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_5px"
                                android:layout_marginEnd="@dimen/_3px"
                                android:layout_marginStart="@dimen/_3px"
                                android:layout_marginTop="@dimen/_5px"
                                android:layout_width="match_parent" style="@style/groupStyledeep_purple_A200">
                                <LinearLayout android:id="@+id/linear4"
                                    android:layout_gravity="center"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:orientation="vertical" style="@style/groupStyleblack_900">
                                    <FrameLayout
                                    android:id="@+id/frame3"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="match_parent" android:layout_width="match_parent">
                                    <ImageView
                                    android:id="@+id/image2"
                                    android:layout_gravity="center|end"
                                    android:layout_height="@dimen/_14px"
                                    android:layout_width="@dimen/_8px" android:src="@drawable/img_shape_4"/>
                                    <View
                                    android:background="@color/black_900"
                                    android:id="@+id/line"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_2px" android:layout_width="match_parent"/>
                                    </FrameLayout>
                                </LinearLayout>
                                <FrameLayout android:id="@+id/frame4"
                                    android:layout_gravity="center"
                                    android:layout_height="wrap_content" android:layout_width="match_parent">
                                    <LinearLayout
                                    android:id="@+id/linear5"
                                    android:layout_gravity="center"
                                    android:layout_height="match_parent"
                                    android:layout_width="match_parent"
                                    android:orientation="vertical" style="@style/groupStyleblack_900">
                                    <FrameLayout
                                    android:id="@+id/frame5"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="match_parent" android:layout_width="match_parent">
                                    <ImageView
                                    android:id="@+id/image3"
                                    android:layout_gravity="center|end"
                                    android:layout_height="@dimen/_14px"
                                    android:layout_width="@dimen/_8px" android:src="@drawable/img_shape_4"/>
                                    <View
                                    android:background="@color/black_900"
                                    android:id="@+id/line1"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_2px" android:layout_width="match_parent"/>
                                    </FrameLayout>
                                    </LinearLayout>
                                </FrameLayout>
                            </FrameLayout>
                        </FrameLayout>
                    </FrameLayout>
                </LinearLayout>
                <FrameLayout android:id="@+id/frame6"
