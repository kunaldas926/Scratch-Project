<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="search7VM" type="com.application.app.modules.search7.data.viewmodel.Search7VM"/>
    </data>
    <LinearLayout android:id="@+id/linear"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtSearch"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.17"
                    android:layout_width="0dp"
                    android:text="@{search7VM.search7Model.txtSearch}" style="@style/textStyleRobotoregular20"/>
                <ImageView android:id="@+id/image"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18px"
                    android:layout_marginStart="@dimen/_50px"
                    android:layout_width="@dimen/_18px" android:src="@drawable/img_icon___airplay"/>
                <ImageView android:id="@+id/image1"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18px"
                    android:layout_marginStart="@dimen/_24px"
                    android:layout_width="@dimen/_18px" android:src="@drawable/img_icon___bell"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:id="@+id/scrollView"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_width="match_parent" android:paddingBottom="@dimen/_10px">
            <LinearLayout android:id="@+id/linear2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_900">
                <FrameLayout android:id="@+id/frame"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <FrameLayout android:id="@+id/frame1"
                        android:layout_gravity="center"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent" style="@style/groupStyle">
                        <FrameLayout android:id="@+id/frame2"
                            android:layout_gravity="center"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent" style="@style/groupStyle">
                            <EditText
                                android:background="@drawable/rectangle_bg_black_900_59_radius_3"
                                android:clickable="true"
                                android:fontFamily="@font/roboto"
                                android:gravity="top|start"
                                android:hint="@string/msg_fin_movies_cas"
                                android:id="@+id/etFinMoviesCas"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_12px"
                                android:layout_marginEnd="@dimen/_16px"
                                android:layout_marginStart="@dimen/_16px"
                                android:layout_marginTop="@dimen/_12px"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_8px"
                                android:paddingStart="@dimen/_16px"
                                android:paddingTop="@dimen/_8px"
                                android:singleLine="true"
                                android:text="@={search7VM.search7Model.etFinMoviesCasValue}"
                                android:textColor="@color/white_A700"
                                android:textColorHint="@color/white_A700"
                                android:textFontWeight="400"
                                android:textSize="@dimen/_12px" android:textStyle="normal"/>
                        </FrameLayout>
                    </FrameLayout>
                </FrameLayout>
                <LinearLayout android:id="@+id/linear3"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle">
                    <LinearLayout android:id="@+id/linear4"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStyle">
                        <View android:background="@color/gray_901"
                            android:id="@+id/line"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_1px" android:layout_width="match_parent"/>
                        <TextView android:gravity="top|center"
                            android:id="@+id/txtTrendingSearch"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_20px"
                            android:layout_marginStart="@dimen/_20px"
                            android:layout_marginTop="@dimen/_24px"
                            android:layout_width="match_parent"
                            android:text="@{search7VM.search7Model.txtTrendingSearch}" style="@style/textStyleRobotoregular24"/>
                        <View android:background="@color/gray_901"
                            android:id="@+id/line1"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_1px"
                            android:layout_marginTop="@dimen/_20px" android:layout_width="match_parent"/>
                    </LinearLayout>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtTreasureIsland"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_marginStart="@dimen/_50px"
                        android:layout_marginTop="@dimen/_16px"
                        android:layout_width="match_parent"
                        android:text="@{search7VM.search7Model.txtTreasureIsland}" style="@style/textStyleRobotoregular14"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtLoveInTrouble"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_marginStart="@dimen/_50px"
                        android:layout_marginTop="@dimen/_16px"
                        android:layout_width="match_parent"
                        android:text="@{search7VM.search7Model.txtLoveInTrouble}" style="@style/textStyleRobotoregular14"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtHotelDeLuna"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_marginStart="@dimen/_50px"
                        android:layout_marginTop="@dimen/_16px"
                        android:layout_width="match_parent"
                        android:text="@{search7VM.search7Model.txtHotelDeLuna}" style="@style/textStyleRobotoregular14"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtTheHeirs"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_marginStart="@dimen/_50px"
                        android:layout_marginTop="@dimen/_16px"
                        android:layout_width="match_parent"
                        android:text="@{search7VM.search7Model.txtTheHeirs}" style="@style/textStyleRobotoregular14"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtWhatSWrongWi"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_marginStart="@dimen/_50px"
                        android:layout_marginTop="@dimen/_16px"
                        android:layout_width="match_parent"
                        android:text="@{search7VM.search7Model.txtWhatSWrongWi}" style="@style/textStyleRobotoregular14"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtMobyDick"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_marginStart="@dimen/_50px"
                        android:layout_marginTop="@dimen/_16px"
                        android:layout_width="match_parent"
                        android:text="@{search7VM.search7Model.txtMobyDick}" style="@style/textStyleRobotoregular14"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtBulletBoy"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_50px"
                        android:layout_marginStart="@dimen/_50px"
                        android:layout_marginTop="@dimen/_16px"
                        android:layout_width="match_parent"
                        android:text="@{search7VM.search7Model.txtBulletBoy}" style="@style/textStyleRobotoregular14"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
        <FrameLayout android:id="@+id/frame3"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyle">
            <LinearLayout android:id="@+id/linear5"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_900">
                <LinearLayout android:id="@+id/linear6"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linear7"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStyle">
                        <ImageView android:id="@+id/image2"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_22px"
                            android:layout_marginEnd="@dimen/_34px"
                            android:layout_marginStart="@dimen/_34px"
                            android:layout_marginTop="@dimen/_8px"
                            android:layout_width="@dimen/_22px" android:src="@drawable/img_left_icon"/>
                        <TextView android:gravity="top|center"
                            android:id="@+id/txtHome"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2px"
                            android:layout_width="match_parent"
                            android:text="@{search7VM.search7Model.txtHome}" style="@style/textStyleRobotoregular12_1"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linear8"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStyle">
                        <ImageView android:id="@+id/image3"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_22px"
                            android:layout_marginEnd="@dimen/_34px"
                            android:layout_marginStart="@dimen/_34px"
                            android:layout_marginTop="@dimen/_8px"
                            android:layout_width="@dimen/_22px" android:src="@drawable/img_icon___globe"/>
                        <TextView android:gravity="top|center"
                            android:id="@+id/txtExplore"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2px"
                            android:layout_width="match_parent"
                            android:text="@{search7VM.search7Model.txtExplore}" style="@style/textStyleRobotoregular12"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linear9"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStyle">
                        <ImageView android:id="@+id/image4"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_22px"
                            android:layout_marginEnd="@dimen/_34px"
                            android:layout_marginStart="@dimen/_34px"
                            android:layout_marginTop="@dimen/_8px"
                            android:layout_width="@dimen/_22px" android:src="@drawable/img_icon___airplay_1"/>
                        <TextView android:gravity="top|center"
                            android:id="@+id/txtChannels"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2px"
                            android:layout_width="match_parent"
                            android:text="@{search7VM.search7Model.txtChannels}" style="@style/textStyleRobotoregular12"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linear10"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStyle">
                        <ImageView android:id="@+id/image5"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_22px"
                            android:layout_marginEnd="@dimen/_34px"
                            android:layout_marginStart="@dimen/_34px"
                            android:layout_marginTop="@dimen/_8px"
                            android:layout_width="@dimen/_22px" android:src="@drawable/img_icon___user"/>
                        <TextView android:gravity="top|center"
                            android:id="@+id/txtUser"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2px"
                            android:layout_width="match_parent"
                            android:text="@{search7VM.search7Model.txtUser}" style="@style/textStyleRobotoregular12"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
