<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="detailPage3RowModel" type="com.application.app.modules.detailpage2.data.model.DetailPage3RowModel"/>
    </data>
    <FrameLayout android:clickable="true" android:id="@+id/frame"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" style="@style/groupStyle">
        <LinearLayout android:id="@+id/linear"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyle">
            <FrameLayout android:id="@+id/frame1"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" style="@style/groupStyle">
                <FrameLayout android:id="@+id/frame2"
                    android:layout_gravity="center"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent" style="@style/groupStyle">
                    <ImageView android:id="@+id/image"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_190px"
                        android:layout_width="@dimen/_120px"
                        android:scaleType="centerCrop"
                        cornerRadius="@{@dimen/_2px}" imageRes="@{@drawable/img_cardbox_7}"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:id="@+id/linear1"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4px"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtDarkPhoenix"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="@{detailPage3RowModel.txtDarkPhoenix}" style="@style/textStyleRobotoregular12"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtEveryHeroHas"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2px"
                    android:layout_width="match_parent"
                    android:text="@{detailPage3RowModel.txtEveryHeroHas}" style="@style/textStyleRobotoregular12"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
